{
  "name": "scoge",
  "version": "0.0.0",
  "scripts": {
    "start": "node server2.js",
    "vite": " npx vite --port=4000",
    "server": "node server.js",
    "server2": "node server2.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "dev2": "concurrently \"vite\" \"npm run server2\"",
    "build": "vite build && npm run copy-assets && npm run copy-assets2",
    "copy-assets": "copyfiles -u 1 'assets/nfts/**/*' 'dist/assets/'",
    "copy-assets2": "copyfiles -u 1 'assets/images/items/*' 'dist/assets/'",
    "serve": "npm run build && vite preview",
    "watch": "ic-dfx-watch",
    "sync": "browser-sync start -sw"
  },
  "devDependencies": {
    "@babylonjs/core": "^7.12.0",
    "@babylonjs/inspector": "^7.12.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.5.0",
    "ic-dfx-utils": "^0.0.14",
    "ic-stoic-identity": "^2.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-motoko": "^0.8.4",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "@babylonjs/loaders": "^7.12.0",
    "@chec/commerce.js": "^2.8.0",
    "@coinbase/wallet-sdk": "^4.0.4",
    "@dfinity/agent": "^1.4.0",
    "@dfinity/auth-client": "^1.4.0",
    "@dfinity/candid": "^1.4.0",
    "@dfinity/principal": "^1.4.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@fleekhq/fleek-storage-js": "^1.0.24",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@power-elements/stripe-elements": "^3.3.0",
    "@psychedelic/dab-js": "^1.6.0-alpha.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@stripe/stripe-js": "^4.0.0",
    "@studio-freight/lenis": "^1.0.27",
    "@vercel/analytics": "^1.3.1",
    "ably": "^2.2.0",
    "audio-context": "^1.0.3",
    "babylonjs": "^7.12.0",
    "babylonjs-loaders": "^7.12.0",
    "bignumber.js": "^9.1.2",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "googleapis": "^140.0.1",
    "gsap": "^3.12.5",
    "ms": "^2.1.3",
